<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane" prefHeight="800.0" prefWidth="600.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="200.0" layoutY="323.0" onMouseClicked="#loadGameSheet" onMouseEntered="#visible" onMouseExited="#notVisible" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pildid/play_c.png" />
         </image>
      </ImageView>
      <ImageView fx:id="ivWhitePlay" disable="true" fitHeight="150.0" fitWidth="200.0" layoutX="197.0" layoutY="323.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@pildid/play1_t.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="197.0" layoutY="643.0" onMouseClicked="#closeGame" onMouseEntered="#visibleExit" onMouseExited="#notVisibleExit" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pildid/exit1_c.png" />
         </image></ImageView>
      <ImageView fx:id="ivWhiteExit" disable="true" fitHeight="150.0" fitWidth="200.0" layoutX="197.0" layoutY="643.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@pildid/exit1_t.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
